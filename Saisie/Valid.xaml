        <TextBox HorizontalAlignment="center" Height="auto" FontSize="30" TextWrapping="Wrap" Text="{Binding Path=Abonne , Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="center" Width="auto"/>
